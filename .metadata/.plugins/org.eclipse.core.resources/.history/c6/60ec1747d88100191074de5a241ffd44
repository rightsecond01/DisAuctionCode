package chap03;

public class Q5 {
	
	static int binSearchX(int[] a,int n, int key) {
		int pl =0;
		int pr = n - 1;
		
		do {
			int pc = (pl + pr)/2;
			for(int i =0;i<pc;i++) {
				if(a[i]==key) {
					pc=i;
					break;
				}
			}
			if(a[pc]==key) {
				
			}
		}
	}

	public static void main(String[] args) {

	}

}
