package com.discodeaucion_ver02;

public class RoomVO {
	
	  private String token      = null; 
	  private String room_title = null; 
	  private int room_type     = 0; 
	  private String master_id  = null; 
	  private int p_limit       = 0; 
	  private int p_current     = 0;
	  private String room_game  = null;
	
	  
	public String getToken() {
		return token;
	}
	public void setToken(String token) {
		this.token = token;
	}
	public String getRoom_title() {
		return room_title;
	}
	public void setRoom_title(String room_title) {
		this.room_title = room_title;
	}
	public int getRoom_type() {
		return room_type;
	}
	public void setRoom_type(int room_type) {
		this.room_type = room_type;
	}
	public String getMaster_id() {
		return master_id;
	}
	public void setMaster_id(String master_id) {
		this.master_id = master_id;
	}
	public int getP_limit() {
		return p_limit;
	}
	public void setP_limit(int p_limit) {
		this.p_limit = p_limit;
	}
	public int getP_current() {
		return p_current;
	}
	public void setP_current(int p_current) {
		this.p_current = p_current;
	}
	public String getRoom_game() {
		return room_game;
	}
	public void setRoom_game(String room_game) {
		this.room_game = room_game;
	}
	

}
